{
  "version": 2,

  "routes": [
    {
      "src": "/",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET"
      },
      "dest": "/api/index.js"
    },
    {
      "src": "/tag/(?<tag>[^/]+)",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET"
      },
      "dest": "/api/tag.js?tag=$tag"
    }
  ]
}
