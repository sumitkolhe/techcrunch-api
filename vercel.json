{
  "version": 2,

  "rewrites": [
    {
      "source": "/",
      "destination": "/api/index.js"
    },
    { "source": "/tag/:tag", "destination": "/api/tag.js" }
  ],

  "headers": [
    {
      "source": "/",
      "headers": [
        {
          "key":"Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
